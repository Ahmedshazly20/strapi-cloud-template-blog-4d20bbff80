{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "Extended user model for educational platform",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true,
      "unique": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 2
    },
    "phone": {
      "type": "string",
      "required": true,
      "maxLength": 20,
      "regex": "^[0-9+\\-\\s()]+$"
    },
    "hasSeenWelcome": {
      "type": "boolean",
      "default": false
    },
    "intelligenceScores": {
      "type": "json",
      "default": null
    },
    "assignedPath": {
      "type": "enumeration",
      "enum": ["linguistic", "logical", "interpersonal"],
      "default": null
    },
    "initialProgrammingScore": {
      "type": "integer",
      "min": 0,
      "max": 100,
      "default": null
    },
    "finalProgrammingScore": {
      "type": "integer",
      "min": 0,
      "max": 100,
      "default": null
    },
    "unitResults": {
      "type": "json",
      "default": null
    },
    "status": {
      "type": "enumeration",
      "enum": ["active", "completed"],
      "default": "active"
    },
    "quiz_results": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz-result.quiz-result",
      "mappedBy": "user"
    },
    "unit_results": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::unit-result.unit-result",
      "mappedBy": "user"
    }
  }
}